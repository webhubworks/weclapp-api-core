{
    "name": "webhubworks/weclapp-api-core",
    "description": "This package houses a generated API client to interact with the Weclapp API.",
    "keywords": [
        "webhubworks",
        "weclapp-api-core"
    ],
    "homepage": "https://github.com/webhubworks/weclapp-api-core",
    "license": "MIT",
    "authors": [
        {
            "name": "Marven Thieme",
            "email": "thieme@webhub.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "pestphp/pest": "^3.0",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Webhubworks\\WeclappApiCore\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webhubworks\\WeclappApiCore\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
